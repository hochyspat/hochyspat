#include <iostream>
#include <cstring>

using namespace std;

int main()
{
    double array1[5];
    int countPositive = 0;
    int countNegative = 0;
    for (int i = 0; i < size(array1); ++i)
    {
        cin >> array1[i];
        if (array1[i] > 0)
            countPositive++;

        if (array1[i] < 0)
            countNegative++;
    }

    cout << "Count Positive: " << countPositive << endl;
    cout << "Count Negative: " << countNegative << endl;
}
