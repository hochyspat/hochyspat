#include <iostream>
#include <cstring>

using namespace std;

int main()
{
    double array1[5];
    double maxx = 0;
    double minn = 10000000;
    for (int i = 0; i < size(array1); ++i)
    {
        cin >> array1[i];
        maxx = max(maxx, array1[i]);
        minn = min(minn, array1[i]);
    }

    cout << maxx - minn << endl;
}
