#include <iostream>
#include <cstdlib>

using namespace std;

int main()
{
	int arr[3][3];
	for (int i = 0; i < 3; ++i)
	{
		for (int j = 0; j< 3; ++j)
			arr[i][j] = rand();
	}
	for (int i = 0; i < 3; ++i)
	{
		for (int j = 0; j < 3; ++j)
		{
			cout << arr[i][j] << " ";
		}
		cout << endl;
	}

	int maxSum = 0;
	int maxIndex = 0;
	for (int i = 0; i < 3; ++i)
	{
		int sumRow = 0;
		for (int j = 0; j < 3; ++j)
		{
			sumRow += arr[i][j];

		}
		if (maxSum < sumRow)
		{
			maxSum = sumRow;
			maxIndex = i;
		}
		cout << "sumRow" << i + 1 << " is " << sumRow << endl;
	}
	cout << maxIndex + 1;
}
