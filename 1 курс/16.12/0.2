#include <iostream>

using namespace std;

int main()
{
	int arr[3][3];
	for (int i = 0; i < 3; ++i)
	{
		for (int j = 0; j < 3; ++j)
		{
			int number;
			cin >> number;
			arr[i][j] = number;
		}
	}
	cout << endl;
	for (int i = 0; i < 3; ++i)
	{
		for (int j = 0; j < 3; ++j)
			cout << arr[i][j] << " ";
		cout << endl;
	}
	for (int i = 0; i < 3; ++i)
	{
		int sumRow = 0;
		int sumColumn = 0;
		for (int j = 0; j < 3; ++j)
		{
			sumRow += arr[i][j];
			sumColumn += arr[j][i];
		}
		cout << "sumRow" << i + 1 << " is " << sumRow << endl;
		cout << "sumColumn" << i + 1 << " is " << sumColumn << endl;
	}
}
