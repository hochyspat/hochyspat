#include <iostream>

using namespace std;

int main()
{
    for (int i = 1000; i < 10000; ++i)
    {
        int digit1 = i % 10;
        int digit2 = i % 100 / 10;
        int digit3 = i / 100 % 10;
        int digit4 = i / 1000;
        int summ = digit1 + digit2 + digit3 + digit4;

        if (summ == 15)
            cout << i << endl;
    }

}
